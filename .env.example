# Data Location Settings
# Directory where data files are stored (default: data)
DATA_DIR=data

# LLM Gateway Settings (OpenAI-compatible API)
LLM_API_KEY=your_api_key_here
LLM_BASE_URL=http://0.0.0.0:8000/v1

# Default Model Settings
DEFAULT_TEMPERATURE=0.5
CACHE_SEED=42

# Embedding Settings
EMBEDDING_MODEL_NAME=Qwen/Qwen3-Embedding-0.6B

# Optional: OpenAI API Key (for specific models)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Gemini API Key (for Google models)
GEMINI_API_KEY=your_gemini_api_key_here
